        <arguments>
          <argument>
            <kind>1</kind>
            <string>///init
if(audio_is_playing(bgm_1)) audio_stop_sound(bgm_1);
else if(audio_is_playing(bgm_2)) audio_stop_sound(bgm_2);
else if(audio_is_playing(bgm_3)) audio_stop_sound(bgm_3);
else if(audio_is_playing(bgm_4)) audio_stop_sound(bgm_4);
else if(audio_is_playing(bgm_5)) audio_stop_sound(bgm_5);
else if(audio_is_playing(bgm_over)) audio_stop_sound(bgm_over);

audio_play_sound(bgm_menu, 19, true);

//var
select = 0;
global.show_fps = false;
global.reduce = false;
x = 100;
y = room_height / 2
start = false;
menu_layer = 0;
vspeed = 0;
title_move = false;
title_y = 240;
hurt = false;
y_slide = 960;

global.dif = 0;

//alarms
alarm[0] = irandom_range(0, 10)

//file
ini_open("data.ini")
global.hscore = ini_read_real("data", "highscore", 0);
global.show_fps = ini_read_real("data", "fps", 0);
global.reduce = ini_read_real("data", "reduce", 0);
ini_close();
</string>
          </argument>
        </arguments>
      </action>
    </event>
    <event eventtype="2" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>vspeed = irandom_range(-5, 5)

alarm[0] = irandom_range(3, 5);
</string>
          </argument>
        </arguments>
      </action>
    </event>
    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>//make star
if(irandom(2) = 1) instance_create(960, irandom_range(90, 454), obj_star);
instance_create(x, y, obj_player_particle);

//control player ghost
if(y &lt;= 80) vspeed = 5;
else if(y &gt;= 460) vspeed = -5;
image_angle = (vspeed / 5) * -45;

if(start) hspeed += 1; 
if(obj_mainmenu.x &gt;= 960) {
    if(audio_is_playing(bgm_menu)) audio_stop_sound(bgm_menu);
    room_goto(rm_play);
}

if(title_move){
    if(title_y &gt; 100) title_y -= 2;
}
else if(!title_move){
    if(title_y &lt; 240) title_y += 2;
}

title_y = clamp(title_y, 100, 240);

if(menu_layer = 3 || menu_layer = 4) title_move = true;
else title_move = false;

//clamp selection
if(menu_layer = 0 || menu_layer = 1 || menu_layer = 2) select = clamp(select, 0, 3);
else if(menu_layer = 3 || menu_layer = 4) select = clamp(select, 3, 3);
else if(menu_layer = 5) select = clamp(select, 2, 3);

//choose
if(gamepad_button_check_pressed(0, gp_padu) &amp;&amp; !start) select -= 1;
else if(gamepad_button_check_pressed(0, gp_padd) &amp;&amp; !start) select += 1;

//main_menu
if(gamepad_button_check_pressed(0, gp_face1) &amp;&amp; select = 0 &amp;&amp; menu_layer = 0) {menu_layer = 1; select = 0;} //dificulty
else if(gamepad_button_check_pressed(0, gp_face1) &amp;&amp; select = 1 &amp;&amp; menu_layer = 0) {menu_layer = 2; select = 0;} //option
else if(gamepad_button_check_pressed(0, gp_face1) &amp;&amp; select = 2 &amp;&amp; menu_layer = 0) {menu_layer = 3; select = 3;} //how to
else if(gamepad_button_check_pressed(0, gp_face1) &amp;&amp; select = 3 &amp;&amp; menu_layer = 0) {menu_layer = 4; select = 3;} //credit

//dif
else if(gamepad_button_check_pressed(0, gp_face1) &amp;&amp; select = 0 &amp;&amp; menu_layer = 1) {
    global.dif = 1;
    start = true;
}
else if(gamepad_button_check_pressed(0, gp_face1) &amp;&amp; select = 1 &amp;&amp; menu_layer = 1) {
    global.dif = 2;
    start = true;
}
else if(gamepad_button_check_pressed(0, gp_face1) &amp;&amp; select = 2 &amp;&amp; menu_layer = 1) {
    global.dif = 3;
    start = true;
}
else if(gamepad_button_check_pressed(0, gp_face1) &amp;&amp; select = 3 &amp;&amp; menu_layer = 1) {menu_layer = 0; select = 0;} //reduce

//option
else if(gamepad_button_check_pressed(0, gp_face1) &amp;&amp; select = 0 &amp;&amp; menu_layer = 2) {
    global.reduce = !global.reduce;
    ini_open("data.ini");
    ini_write_real("data", "reduce", global.reduce);
    ini_close();
} //reduce
else if(gamepad_button_check_pressed(0, gp_face1) &amp;&amp; select = 1 &amp;&amp; menu_layer = 2) {
    global.show_fps = !global.show_fps;
    ini_open("data.ini");
    ini_write_real("data", "fps", global.show_fps);
    ini_close();
} //fps
else if(gamepad_button_check_pressed(0, gp_face1) &amp;&amp; select = 2 &amp;&amp; menu_layer = 2) {menu_layer = 5; select = 2;} //back
else if(gamepad_button_check_pressed(0, gp_face1) &amp;&amp; select = 3 &amp;&amp; menu_layer = 2) {menu_layer = 0; select = 0;} //back

//how to
else if(gamepad_button_check_pressed(0, gp_face1) &amp;&amp; select = 3 &amp;&amp; menu_layer = 3) {menu_layer = 0; select = 0;} //reduce

//credit
else if(gamepad_button_check_pressed(0, gp_face1) &amp;&amp; select = 3 &amp;&amp; menu_layer = 4) {menu_layer = 0; select = 0;} //reduce

//sure
else if(gamepad_button_check_pressed(0, gp_face1) &amp;&amp; select = 2 &amp;&amp; menu_layer = 5) {menu_layer = 2; select = 0;}
else if(gamepad_button_check_pressed(0, gp_face1) &amp;&amp; select = 3 &amp;&amp; menu_layer = 5) {
    ini_open("data.ini");
    ini_write_real("data", "highscore", 0);
    ini_close();
    room_restart();
} //delete sure

if(select = 2 &amp;&amp; menu_layer = 1) hurt = true;
else hurt = false;

if(hurt) y_slide -= 20;
else if(!hurt) y_slide += 20;

y_slide = clamp(y_slide, 140, 960);
</string>
          </argument>
        </arguments>
      </action>
